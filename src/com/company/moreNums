    public static void num7() {
        for (int i=0; i<5; i++) {
            for (int j=0; j<3; j++) {
                System.out.print(i);
            }
            //System.out.println();
        }
    }
    
    public static void num8() {
        for (int i=1; i<=9; i++) {
            for (int j=0; j<i; j++) {
                System.out.print(i);
            }
        }
    }
    
    /*
